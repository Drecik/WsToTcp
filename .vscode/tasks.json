{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "dotnet run",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"run"
			],
			"isBackground": false,
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build"
		},
		{
			"label": "run-proxy",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"run",
				"--urls",
				"http://0.0.0.0:5000"
			],
			"isBackground": true,
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build"
		}
	]
}